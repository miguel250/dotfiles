[color]
    diff = auto
    status = auto
    branch = auto
    ui = true

[alias]
    co = checkout
    count = !git shortlog -sn

[apply]
    whitespace = nowarn

[mergetool]
    keepBackup = false

[difftool]
    prompt = false

[color "branch"]
	current = yellow reverse
	local = yellow bold
	remote = green bold
	plain = red bold

[help]
    autocorrect = 1

[push]
    default = simple
